<div>
  <h1 class="card z-depth-3 center-align">Read the Latest News</h1>
  <div class="row">
    <div class="col s3"></div>
    <h5> Click 'Scrape' to get the latest articles</h5>
  </div>
</div>


{{#each article}}
<div id="card" class="card ">
  <div class="card-body">
    <h3 class="card-title">{{this.title}}</h3>
    <a id="readmore" href="/readArticle/{{this._id}}" class="">read more...</a>
  </div>

</div> 
{{/each}}